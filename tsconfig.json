{
  "compilerOptions": {
    "target": "es2018",
    "module": "CommonJS",
   "lib": ["es2020", "esnext.array", "DOM"],
    "declaration": true,
    "outDir": "./",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["index.ts"],
  "exclude": ["node_modules"]
}
